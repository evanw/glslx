# Must assign to a location with storage
test("
void main(int x, vec2 y) {
  // Bad
  0 = 0;
  vec2(0.0) = vec2(0.0);
  vec2(0.0).y = 0.0;
  vec2(0.0)[0] = 0.0;

  // Good
  x = 0;
  y = vec2(0.0);
  y.y = 0.0;
  y[0] = 0.0;

  // Bad
  0 += 0;
  vec2(0.0) += vec2(0.0);
  vec2(0.0).y += 0.0;
  vec2(0.0)[0] += 0.0;

  // Good
  x += 0;
  y += vec2(0.0);
  y.y += 0.0;
  y[0] += 0.0;

  // Bad
  vec2(0.0)[0]++;
  --vec2(0.0)[0];

  // Good
  y[0]++;
  --y[0];
}
", "
